COMPOSE_PROJECT_NAME=development
# this is to set the stack for each group of services
PUID=1000
# Main linux user id that will be running server
PGID=994
# docker group id
TZ=America/Winnipeg
# this is your time zone
USERDIR={set to directory you would like}/server/config/LEMP # for me this is one the shares on open media vault
# this will be the location of the persistent data for all services running on the server
# change {set to directory you would like} to the directory you would like the persistent data to be saved
MYSQL_ROOT_PASSWORD=root
# this will be the main password for both mariadb and phpmyadmin
